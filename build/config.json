{
    "database": {
        "path": "../database/db.sqlite"
    },
    "log": {
        "path": "../logs/",
        "maxLines": 200,
        "levels": {
            "debug": true,
            "server": true,
            "conn": true,
            "info": true,
            "warn": true,
            "error": true
        }
    },
    "connection": {
        "port": 8443,
        "secure": true,
        "cert": "/etc/letsencrypt/live/jomity.net/cert.pem",
        "privkey": "/etc/letsencrypt/live/jomity.net/privkey.pem",
        "interface": "eth0"
    }
}
